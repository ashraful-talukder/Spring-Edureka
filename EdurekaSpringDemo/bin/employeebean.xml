<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="emp1" class="co.edureka.Employee">
		<property name="eid" value="102"/>
		<property name="ename" value="Jonathan"/>
		<property name="esalary" value="15000"/>
	</bean>

	<bean id="emp2" class="co.edureka.Employee">
		<property name="eid" value="103"/>
		<property name="ename" value="James"/>
		<property name="esalary" value="35000"/>
	</bean>

</beans>